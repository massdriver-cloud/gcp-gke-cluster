[OUTPUT]
    Name  opensearch
    Match kube.*
    Host  opensearch-cluster-master.${namespace}.svc.cluster.local
    Port  9200
    HTTP_User %%{${username}}
    HTTP_Passwd %%{${password}}
    Logstash_Format True
    Logstash_Prefix "k8s-pod-logs"
    Retry_Limit False
    TLS on
    tls.verify off
[OUTPUT]
    Name  opensearch
    Match host.*
    Host  opensearch-cluster-master.${namespace}.svc.cluster.local
    Port  9200
    HTTP_User %%{${username}}
    HTTP_Passwd %%{${password}}
    Logstash_Format True
    Logstash_Prefix "k8s-node-logs"
    Retry_Limit False
    TLS on
    tls.verify off