# adapted from https://opensearch.org/docs/latest/security-plugin/configuration/yaml/#internal_usersyml
---
# This is the internal user database
# The hash value is a bcrypt hash and can be generated with plugin/tools/hash.sh

_meta:
  type: internalusers
  config_version: 2

# Define your internal users here
fluentbit:
  hash: ${password}
  reserved: false
  hidden: false
  backend_roles:
  - all_access
  static: false
